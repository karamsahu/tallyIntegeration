<ENVELOPE>
<HEADER>
	<TALLYREQUEST>Import Data</TALLYREQUEST>
</HEADER>
<BODY>
<IMPORTDATA>
	<REQUESTDESC>
	<REPORTNAME>All Masters</REPORTNAME>
</REQUESTDESC>
<REQUESTDATA>
<!-- Create Stock Item named "" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<STOCKITEM NAME="$stockItemName"ACTION="$action">
	<NAME.LIST>
		<NAME>$stockItemName</NAME> 
	</NAME.LIST>
	<ADDITIONALNAME.LIST>
		<ADDITIONALNAME></ADDITIONALNAME> 
	</ADDITIONALNAME.LIST>
	<PARENT>$stockItemParent</PARENT>
	<BASEUNITS>NOS</BASEUNITS>
	<OPENINGBALANCE>$stockOpeningBalance</OPENINGBALANCE>
	<OPENINGVALUE>-$stockOpenginValue</OPENINGVALUE>
	<OPENINGRATE>$stockOpeningRate</OPENINGRATE>
	<BATCHALLOCATIONS.LIST>
		<NAME></NAME>
		<BATCHNAME></BATCHNAME>
		<GODOWNNAME></GODOWNNAME>
		<MFDON></MFDON>
		<OPENINGBALANCE></OPENINGBALANCE>
		<OPENINGVALUE></OPENINGVALUE>
		<OPENINGRATE></OPENINGRATE>
	</BATCHALLOCATIONS.LIST>
	<STANDARDPRICELIST.LIST>
		<RATE></RATE>
		<DATE></DATE>
	</STANDARDPRICELIST.LIST>
	<STANDARDCOSTLIST.LIST>
		<RATE></RATE>
		<DATE></DATE>
	</STANDARDCOSTLIST.LIST>
	<REORDERBASE></REORDERBASE>
	<MINIMUMORDERBASE></MINIMUMORDERBASE>
</STOCKITEM>
</TALLYMESSAGE>
</REQUESTDATA>
</IMPORTDATA>
</BODY>
</ENVELOPE>